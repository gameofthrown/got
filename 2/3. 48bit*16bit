 
N1 DW   2 DUP(?)
N2 DW   3 DUP(?)
N3 DW   4 DUP(0)

;MULTIPLICAND IS 1234FF54FF23H 
;MULTIPLIER IS F123H
MOV N1,0f123H 

MOV N2,0ff23H
MOV N2+2,0FF54H
MOV N2+4,1234h 
MOV AX,N1
mul N2 
MOV N3,ax
MOV N3+2,DX 
MOV AX,N1
mul N2+2
ADD N3+2,AX
ADC N3+4,DX
MOV AX,N1
MUL N2+4 
ADC N3+4,AX
ADC N3+6,DX
 
HLT

 

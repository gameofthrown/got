MOV BX,1100H
MOV DI,1120H
MOV AX,[BX]  
XCHG AH, AL
MOV CX,[BX+2]
XCHG cH, cL
CALL GCD
MOV AX,[BX] 
XCHG AH, AL
MOV CX,[BX+2]
XCHG cH, cL
MUL CX
DIV SI
MOV [DI],ah
mov [di+1],al
HLT
GCD PROC NEAR
 RPT:
 CMP AX,CX
 JE SAVE
 JNC SKIP
 XCHG AX,CX
 SKIP:
 MOV DX,0000H
 DIV CX
 CMP DX,0000H
 JE SAVE
 MOV AX,DX
 JMP RPT
 SAVE:
 MOV SI,CX
 RET
 GCD ENDP
